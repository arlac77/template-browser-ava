{
  "scripts": {
    "test": "npm run test:browser-ava",
    "test:browser-ava": "browser-ava {{browser-ava.options}} {{browser-ava.tests}}"
  },
  "devDependencies": {
    "browser-ava": "^1.0.3"
  },
  "template": {
    "properties": {
      "browser-ava": {
        "options": "--no-keep-open",
        "tests": "tests/*-ava.mjs tests/*-ava-browser.mjs"
      }
    },
    "inheritFrom": [
      "arlac77/template-javascript"
    ],
    "usedBy": [
      "arlac77/content-entry-transform",
      "arlac77/iterable-string-interceptor",
      "arlac77/key-value-transformer",
      "arlac77/matching-iterator",
      "arlac77/reader-line-iterator",
      "arlac77/repository-provider"
    ]
  }
}